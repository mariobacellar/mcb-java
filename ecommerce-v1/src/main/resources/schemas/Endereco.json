{
	"Endereco": {

		"type": "object",
		"properties": {
			"id": {
			"type": "string",
			"format": "uuid",
			"description": "Identificador único do endereço"
			},
			"clienteId": {
			"type": "string",
			"format": "uuid",
			"description": "Referência ao ID do cliente"
			},
			"cep": {
			"type": "string",
			"description": "CEP do endereço"
			},
			"logradouro": {
			"type": "string",
			"description": "Logradouro (rua, avenida, etc)"
			},
			"numero": {
			"type": "string",
			"description": "Número do endereço"
			},
			"complemento": {
			"type": "string",
			"description": "Complemento do endereço"
			},
			"bairro": {
			"type": "string",
			"description": "Bairro"
			},
			"cidade": {
			"type": "string",
			"description": "Cidade"
			},
			"estado": {
			"type": "string",
			"description": "Estado (UF)"
			},
			"pais": {
			"type": "string",
			"description": "País"
			},
			"tipoEndereco": {
			"type": "string",
			"enum": ["RESIDENCIAL", "COMERCIAL", "ENTREGA", "COBRANCA"],
			"description": "Tipo do endereço"
			},
			"principal": {
			"type": "boolean",
			"description": "Indica se é o endereço principal"
			}
		},
		"required": ["id", "clienteId", "cep", "logradouro", "numero", "bairro", "cidade", "estado", "tipoEndereco"]
	}
}
