{	
	"Newsletter": {
	  "type": "object",
	  "properties": {
		"id": {
		  "type": "string",
		  "format": "uuid",
		  "description": "Identificador único do cadastro"
		},
		"email": {
		  "type": "string",
		  "format": "email",
		  "description": "Email cadastrado"
		},
		"nome": {
		  "type": "string",
		  "description": "Nome da pessoa"
		},
		"dataCadastro": {
		  "type": "string",
		  "format": "date-time",
		  "description": "Data do cadastro"
		},
		"status": {
		  "type": "string",
		  "enum": ["ATIVO", "INATIVO", "CANCELADO"],
		  "description": "Status da inscrição"
		},
		"origem": {
		  "type": "string",
		  "description": "Origem do cadastro (rodapé, popup, etc.)"
		},
		"ultimoEnvio": {
		  "type": "string",
		  "format": "date-time",
		  "description": "Data do último envio"
		},
		"preferencias": {
		  "type": "array",
		  "items": {
			"type": "string"
		  },
		  "description": "Preferências de conteúdo"
		}
	  },
	  "required": ["id", "email", "dataCadastro", "status"]
	}
}
